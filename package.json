{
  "name": "typekind",
  "version": "0.0.4",
  "author": "Nate Buckareff",
  "description": "Strongly typed JSON serialization",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/natebuckareff/typekind"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "typeck": "tsc --noEmit",
    "build": "pnpm run clean && tsc -p .",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build && pnpm run typeck"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.1",
    "@types/node": "^24.0.13",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
